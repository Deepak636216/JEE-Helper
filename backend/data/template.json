  "official_solution": {
    "steps": [
      {
        "step_number": 1,
        "description": "Calculate deceleration of bullet",
        "formula": "a = F/m",
        "calculation": "a = -2.5×10⁻²/(20×10⁻³) = -5/4 m/s²",
        "explanation": "Negative sign because resistance opposes motion"
      },
      {
        "step_number": 2,
        "description": "Apply equation of motion",
        "formula": "v² = u² + 2as",
        "calculation": "v² = 1² + 2×(-5/4)×(20×10⁻²)",
        "explanation": "Convert units: 20 cm = 0.2 m"
      },
      {
        "step_number": 3,
        "description": "Solve for final velocity",
        "calculation": "v² = 1 - 1/2 = 1/2",
        "result": "v = 1/√2 ≈ 0.7 m/s"
      }
    ],
    "answer_justification": "Option (d) is correct"
  },
  
  "common_mistakes": [
    {
      "mistake": "Forgetting to convert units",
      "consequence": "Wrong numerical answer",
      "correct_approach": "Convert to SI units: 20g → 0.02kg, 20cm → 0.2m"
